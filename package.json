    "@remix-run/react": "^1.15.0",
    "@remix-run/serve": "^1.15.0",
    "@remix-run/vercel": "^1.15.0",
    "@sanity/client": "^5.4.2",
    "@sanity/document-internationalization": "github:EmilEinarsen/document-internationalization",
    "@sanity/image-url": "^1.0.2",
    "@sanity/preview-kit": "^1.4.0",
    "@sanity/vision": "^3.8.3",
    "@vercel/analytics": "^0.1.11",
    "clsx": "^1.2.1",
    "groq": "^3.8.3",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^2.1.0",
    "react-is": "^18.2.0",
    "remix-utils": "^6.1.0",
    "sanity": "^3.9.0",
    "sanity-image": "^0.1.4",
    "sanity-page-builder": "^0.0.1",
    "sanity-plugin-iframe-pane": "^2.3.0",
    "sanity-plugin-media": "^2.0.5",
    "styled-components": "^5.3.9"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@sanity/eslint-config-studio": "^2.0.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@types/react-google-recaptcha": "^2.1.5",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.0",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^11.2.2",
    "prettier": "2.8.8",
    "remix-flat-routes": "^0.5.8",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4"
  },
  "engines": {
